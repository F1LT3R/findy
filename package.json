{
  "name": "findy",
  "version": "1.1.0",
  "description": "Find files really easily.",
  "main": "findy.js",
  "scripts": {
    "test": "xo; mocha *.spec.js"
  },
  "bin": {
    "findy": "findy.js"
  },
  "repository": "git@github.com:f1lt3r/findy.git",
  "author": "f1lt3r",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "micromatch": "^3.0.3",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "mocha": "^3.4.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "globals": [
      "Array",
      "describe",
      "__dirname"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "space-before-function-paren": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-extend-native": 0
    }
  }
